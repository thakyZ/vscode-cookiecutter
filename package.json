{
  "name": "cookiecutter",
  "displayName": "Cookiecutter",
  "description": "Just launch cookiecutter for project creation",
  "version": "0.1.4",
  "publisher": "gxiao",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.66.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension-node.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.cookiecutter",
        "title": "Cookiecutter ..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder && !inDiffEditor",
          "command": "extension.cookiecutter"
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack --mode development",
    "watch": "webpack --watch --mode development",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -watch -p ./",
    "lint": "eslint -c package.json --ext .ts src vscode-bookmarks-core vscode-whats-new"
  },
  "devDependencies": {
    "@types/node": "^14.17.27",
    "@types/vscode": "^1.73.0",
    "@types/mocha": "^9.0.0",
    "@types/glob": "^7.1.4",
    "@vscode/test-electron": "^1.6.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-vscode-ext": "^1.1.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "mocha": "^9.1.3"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "vscode-ext-codicons": "^1.0.0",
    "vscode-ext-decoration": "1.1.0",
    "vscode-ext-help-and-feedback-view": "^1.0.1",
    "vscode-ext-selection": "1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "vscode-ext"
    ]
  },
  "capabilities": {
    "virtualWorkspaces": false,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "homepage": "https://github.com/thakyz/vscode-cookiecutter",
  "repository": {
    "type": "git",
    "url": "https://github.com/thakyz/vscode-cookiecutter.git"
  },
  "bugs": {
    "url": "https://github.com/thakyz/vscode-cookiecutter/issues"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "vscode",
    "mergetools",
    "difftools",
    "merge conflict",
    "conflict",
    "extensions"
  ],
  "__metadata": {
    "publisherDisplayName": "Gong Xiao"
  }
}